node {

    stage('checkout') {
        checkout scmGit(branches: [[name: 'master']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Constelaciones99/cachina.git']])
    }
   
    stage('checkout') {
        sh 'echo "Estamos en la etapa del cambio, perros!"'
        sh 'ls -la'
    }

    stage('Build') {
        sh 'npm install'
    }
   
}
